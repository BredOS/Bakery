<interface>
  <!-- interface-name packages_screen.ui -->
  <requires lib="gtk" version="4.18"/>
  <requires lib="libadwaita" version="1.3"/>
  <object class="AdwBin">
    <property name="can-target">False</property>
  </object>
  <template class="packages_screen" parent="GtkBox">
    <child>
      <object class="AdwStatusPage">
        <property name="description">Don't worry we've got you covered heres a comprehensive list of applications you can install simply tick the application you want installed</property>
        <property name="hexpand">True</property>
        <property name="overflow">hidden</property>
        <property name="title">Need additional apps?</property>
        <property name="valign">center</property>
        <child>
          <object class="GtkBox" id="packages_box">
            <property name="margin-bottom">12</property>
            <property name="margin-end">12</property>
            <property name="margin-start">12</property>
            <property name="margin-top">12</property>
            <property name="orientation">vertical</property>
            <property name="spacing">12</property>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>
